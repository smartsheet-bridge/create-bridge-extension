{
  "extends": "semantic-release-monorepo",
  "branches": [
    "master",
    "next",
    {
      "name": "beta",
      "prerelease": true
    },
    {
      "name": "alpha",
      "prerelease": true
    },
    {
      "name": "release",
      "prerelease": true,
      "channel": false
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/npm",
    "@semantic-release/release-notes-generator",
    "@semantic-release/gitlab"
  ]
}
